@article{zhang_two-stage_2022,
	title = {A {Two}-{Stage} {Path} {Planning} {Algorithm} {Based} on {Rapid}-{Exploring} {Random} {Tree} for {Ships} {Navigating} in {Multi}-{Obstacle} {Water} {Areas} {Considering} {COLREGs}},
